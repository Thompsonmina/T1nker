#import "Counter.jsligo" "Counter"



const test_add = (() => {
  const initial_storage = 10 as int;
  const orig = Test.Next.Originate.contract(contract_of(Counter.Counter), initial_storage, 0tez);
  Test.Next.Contract.transfer_exn(Test.Next.Typed_address.get_entrypoint("increment", orig.taddr), 32 as int, 0tez);
  return Assert.assert(Test.Next.Typed_address.get_storage(orig.taddr) == initial_storage + 32);
}) ()

const test_decrement = (() => {
  const initial_storage = 10 as int;
  const orig = Test.Next.Originate.contract(contract_of(Counter.Counter), initial_storage, 0tez);
  Test.Next.Contract.transfer_exn(Test.Next.Typed_address.get_entrypoint("decrement", orig.taddr), 32 as int, 0tez);
  return Assert.assert(Test.Next.Typed_address.get_storage(orig.taddr) == initial_storage - 32);
}) ()
